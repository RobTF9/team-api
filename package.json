{
  "name": "mern-template",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-mongodb-session": "^2.4.4",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "concurrently": "^7.3.0",
    "cypress": "^10.4.0",
    "eslint": "^8.21.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "kill-port": "^2.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.0.10",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "connect-mongodb-session": "^3.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "mongodb": "^4.8.1",
    "mongoose": "^6.5.0",
    "morgan": "^1.10.0",
    "parcel": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5"
  },
  "scripts": {
    "build:server": "babel server --out-dir dist/server --extensions .ts",
    "build:client": "parcel build client/index.html --dist-dir dist/client",
    "build": "yarn build:server && yarn build:client",
    "prebuild": "yarn clean",
    "dev:client": "parcel serve client/index.html --dist-dir dist/client",
    "dev:server": "nodemon",
    "predev": "yarn kill && yarn clean",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "start": "node dist/server/index.js",
    "pretest": "yarn reset && yarn seed && yarn build",
    "cy:run": "cypress run",
    "test": "PORT=4000 NODE_ENV=test start-server-and-test 'yarn start' http://localhost:4000 'yarn cy:run'",
    "clean": "rimraf .parcel-cache dist",
    "new": "node scripts/component.js",
    "kill": "kill-port 3000",
    "reset": "mongosh mern --eval \"db.dropDatabase();\"",
    "seed": "node scripts/seed.js"
  },
  "alias": {
    "process": false
  }
}
